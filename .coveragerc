[run]
source = .
omit = 
    tests/*
    __pycache__/*
    */__pycache__/*
    .pytest_cache/*
    htmlcov/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
