{
  "openapi": "3.1.0",
  "info": {
    "title": "FastAPI",
    "version": "0.1.0"
  },
  "paths": {
    "/game/start": {
      "post": {
        "summary": "Start Game",
        "operationId": "start_game_game_start_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/StartGameRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BoardState"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/game/status": {
      "get": {
        "summary": "Get Game Status",
        "operationId": "get_game_status_game_status_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BoardState"
                }
              }
            }
          }
        }
      }
    },
    "/game/move": {
      "post": {
        "summary": "Make Move",
        "operationId": "make_move_game_move_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/MoveRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BoardState"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/agents": {
      "get": {
        "summary": "Get Available Agents",
        "operationId": "get_available_agents_agents_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AvailableAgentsResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AvailableAgentsResponse": {
        "properties": {
          "agents": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "title": "Agents"
          }
        },
        "type": "object",
        "required": [
          "agents"
        ],
        "title": "AvailableAgentsResponse"
      },
      "BoardState": {
        "properties": {
          "board": {
            "items": {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            "type": "array",
            "maxItems": 3,
            "minItems": 3,
            "title": "Board"
          },
          "current_player": {
            "type": "string",
            "title": "Current Player"
          },
          "winner": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Winner"
          },
          "winner_line": {
            "anyOf": [
              {
                "prefixItems": [
                  {
                    "prefixItems": [
                      {
                        "type": "integer"
                      },
                      {
                        "type": "integer"
                      }
                    ],
                    "type": "array",
                    "maxItems": 2,
                    "minItems": 2
                  },
                  {
                    "prefixItems": [
                      {
                        "type": "integer"
                      },
                      {
                        "type": "integer"
                      }
                    ],
                    "type": "array",
                    "maxItems": 2,
                    "minItems": 2
                  },
                  {
                    "prefixItems": [
                      {
                        "type": "integer"
                      },
                      {
                        "type": "integer"
                      }
                    ],
                    "type": "array",
                    "maxItems": 2,
                    "minItems": 2
                  }
                ],
                "type": "array",
                "maxItems": 3,
                "minItems": 3
              },
              {
                "type": "null"
              }
            ],
            "title": "Winner Line"
          },
          "game_over": {
            "type": "boolean",
            "title": "Game Over"
          }
        },
        "type": "object",
        "required": [
          "board",
          "current_player",
          "winner",
          "winner_line",
          "game_over"
        ],
        "title": "BoardState"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "MoveRequest": {
        "properties": {
          "row": {
            "type": "integer",
            "maximum": 2.0,
            "minimum": 0.0,
            "title": "Row"
          },
          "col": {
            "type": "integer",
            "maximum": 2.0,
            "minimum": 0.0,
            "title": "Col"
          }
        },
        "type": "object",
        "required": [
          "row",
          "col"
        ],
        "title": "MoveRequest"
      },
      "StartGameRequest": {
        "properties": {
          "human_player_symbol": {
            "anyOf": [
              {
                "type": "string",
                "enum": [
                  "X",
                  "O"
                ]
              },
              {
                "type": "null"
              }
            ],
            "title": "Human Player Symbol"
          },
          "player_x_type": {
            "type": "string",
            "title": "Player X Type"
          },
          "player_o_type": {
            "type": "string",
            "title": "Player O Type"
          }
        },
        "type": "object",
        "required": [
          "player_x_type",
          "player_o_type"
        ],
        "title": "StartGameRequest"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      }
    }
  }
}